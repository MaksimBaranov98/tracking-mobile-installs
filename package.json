{
  "name": "tracking-mobile-installs",
  "version": "1.0.0",
  "author": "Soramitsu",
  "description": "Tracking mobile instals",
  "main": "index.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "yarn clean && tsc && yarn copy-files",
    "build:watch": "yarn clean && yarn copy-files && tsc --watch",
    "start": "yarn run build && yarn copy-files && node ./dist/index.js",
    "serve": "nodemon ./dist/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "4.21.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^22.7.9",
    "@types/pg": "^8.11.10",
    "copyfiles": "^2.4.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.1.7",
    "prettier": "2.8.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@3.4.1"
}
